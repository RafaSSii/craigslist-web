<div id="<%= dom_id listing %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= listing.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= listing.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Images:</strong>
    <% listing.images.each do |image| %>
      <div><%= link_to image.filename, image, class: "text-gray-700 underline hover:no-underline" %></div>
    <% end %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= listing.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Price:</strong>
    <%= listing.price %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Location:</strong>
    <%= listing.location %>
  </div>
  <div>
    <strong class="block font-medium mb-1">X:</strong>
    <%= listing.x %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Y:</strong>
    <%= listing.y %>
  </div>
</div>
